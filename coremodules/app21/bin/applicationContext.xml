<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:c="http://www.springframework.org/schema/c" 
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

 
    <bean id="account" class="com.durgasoft.beans.Account" c:accNo="abc123" c:accName="AAA" c:accType="saving" c:balance="2000" />
    
         
      <bean id="empBean" class="com.durgasoft.beans.Employee"  c:eid="e-11" c:ename=="BBB" c:esal="1000" c:eaddr="hyd" c:acc-ref="account" />
                     
</beans>